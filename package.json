{
  "name": "dxrmb",
  "version": "0.0.3",
  "author": "bh",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/dxrmb.esm.js",
  "keywords": [
    "rmb",
    "dxrmb",    
    "人民币大写"
  ],
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --watch",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/mylib3.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/mylib3.esm.js",
      "limit": "10 KB"
    }
  ],
  "peerDependencies": {
    "decimal.js": ">=10"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.1",
    "husky": "^9.0.11",
    "size-limit": "^11.1.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.2",
    "decimal.js": "^10.4.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hohogpb/dxrmb.git"
  },
  "bugs": {
    "url": "https://github.com/hohogpb/dxrmb/issues"
  },
  "homepage": "https://github.com/hohogpb/dxrmb#readme"
}
